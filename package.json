{
  "name": "dreamliner-backend-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:deploy:dev": "npm i -f && nest build && sls deploy --aws-profile dreamliner_lambda --stage dev",
    "build:deploy:prod": "nest build && sls deploy --aws-profile dreamliner_lambda --stage prod",
    "start:dev": "nest start --watch",
    "format": "prettier --write \"src/*/.ts\" \"test/*/.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:production": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/*/.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@fastify/aws-lambda": "^3.3.0",
    "@fastify/compress": "^6.4.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/csrf-protection": "^6.3.0",
    "@fastify/static": "^6.10.2",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.3",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.0.3",
    "@nestjs/platform-fastify": "^10.0.3",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^7.0.12",
    "async": "^3.2.4",
    "aws-lambda": "^1.0.7",
    "aws-lambda-fastify": "^2.2.0",
    "aws-sdk": "^2.1410.0",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "fastify": "^4.18.0",
    "moment": "^2.29.4",
    "mongoose": "^7.3.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rxjs": "^7.8.1",
    "@types/aws-lambda": "^8.10.119",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/moment": "^2.13.0",
    "@types/node": "20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.5",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@types/aws-lambda": "^8.10.119",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/moment": "^2.13.0",
    "@types/node": "20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-offline": "^12.0.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
